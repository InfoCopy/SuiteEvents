<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:SuiteEvents.Modules.MainMenu.MainMenu.Views"
                    xmlns:viewModels="clr-namespace:SuiteEvents.Modules.MainMenu.MainMenu.ViewModels">

    <DataTemplate DataType="{x:Type viewModels:MainMenuViewModel}">
        <views:MainMenuView/>
    </DataTemplate>
    
    <Style x:Key="FieldLabelStyle" TargetType="Label">
        <Style.Setters>
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Right" />
            <Setter Property="Margin" Value="0,0,10,0" />
        </Style.Setters>
    </Style>

</ResourceDictionary>